{% if id == '_komplet_sidebar_widgets' %}
    {% if display_id == 'block_on_sidebar_latest_articles' %}
        <div>
            {{ rows }}
        </div>
    {% elseif display_id == 'block_on_sidebar_latest_products' %}
        <div class="widget">
            {{ header }}
            <div>
                {{ rows }}
            </div>
        </div>
    {% else %}
        {{ header }}
        {{ rows }}
        {{ attachment_after }}
        {{ empty }}
        {{ footer }}
        {{ pager }}
    {% endif %}
{% elseif id == '_komplet_taxonomy_terms' %}
    {% if display_id == 'block_on_sidebar_latest_tags' %}
        <div class="tagcloud komplet-tags">
            {{ rows }}
        </div>
    {% elseif display_id == 'block_footer_column_3_latest_tags' %}
        <div class="tagcloud komplet-tags-footer">
            {{ rows }}
        </div>
    {% elseif display_id == 'block_portfolio_categories_1' %}
        <ul class="portfolio-filter mb30 list-inline wow">
            {{ header }}
            {{ rows }}
        </ul>
    {% elseif display_id == 'block_portfolio_categories_2' %}
        <ul class="masonry-portfolio-filters mb30 list-inline wow">
            {{ header }}
            {{ rows }}
        </ul>
    {% elseif display_id == 'block_portfolio_categories_3' %}
        <ul class="portfolio-filter mb30 list-inline wow">
            {{ header }}
            {{ rows }}
        </ul>
    {% elseif display_id == 'block_taxonomy_term_food_and_drink_categories' %}
        <ul class="nav nav-justified" id="menu-tabs" role="tablist">
            {{ rows }}
        </ul>

    {% endif %}

