                {% for i, row in rows.0['#rows'] %}
                    {% if temp == 1 %}
                        <div class="row mb50">
                        {{ row }}
                        {% set temp = temp + 1 %}
                    {% elseif temp == 2 %}
                        {{ row }}
                        {% set temp = temp + 1 %}
                    {% elseif temp == 3 or i == (rows.0['#rows']|length - 1) %}
                        {{ row }}
                        </div>
                        {% set temp = 1 %}
                    {% endif %}
                {% endfor %}
